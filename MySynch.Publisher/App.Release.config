<?xml version="1.0" encoding="utf-8" ?>
<!-- For more information on using transformations 
     see the web.config examples at http://go.microsoft.com/fwlink/?LinkId=214134. -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
    <appSettings>
    <add key="BrokerUrl" value="http://sciendo-laptop/broker/" xdt:Locator="Match(key)" xdt:Transform="Replace"/>
    <add key="InstanceName" value="publisher" xdt:Locator="Match(key)" xdt:Transform="Replace"/>
    <add key="ConsiderOfflineChnages" value="no" xdt:Locator="Match(key)" xdt:Transform="Replace"/>
    </appSettings>

</configuration>